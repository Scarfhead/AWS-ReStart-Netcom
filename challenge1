def is_prime(NUMBER):
    ODD = 2
    while ODD < NUMBER:
        while ODD < 250:
            ODD += 1
            if NUMBER % ODD >= 1:
                return True
            else:
                return False
        
        

NUMBER = 19
list = []
for i in range(250):
    if not is_prime(NUMBER):
        print("OK")
        NUMBER += 1
        i += 1
        list.append(NUMBER)
    else:
        print("nice")
        NUMBER += 1
        i += 1
        list.append(NUMBER)

print(list)
